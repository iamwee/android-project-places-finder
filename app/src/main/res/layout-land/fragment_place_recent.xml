<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_recent_place"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:paddingBottom="@dimen/recycler_view_all_padding"
        android:paddingEnd="@dimen/recycler_view_landscape_padding"
        android:paddingStart="@dimen/recycler_view_landscape_padding"
        android:paddingTop="@dimen/recycler_view_all_padding" />

    <ViewStub
        android:id="@+id/view_stub"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout="@layout/partial_place_not_found" />


</RelativeLayout>